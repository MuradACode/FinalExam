@model Settings

<section class="container">
    <div class="card">
                <div class="card-body">
                  <h4 class="card-title">Feature - Update</h4>
                  <form method="post" class="forms-sample">
                    <div class="form-group">
                      <label for="settingsKey">Settings Key</label>
                      <input name="settingsKey" value="@Model.Key" id="settingsKey" class="form-control">
                      <span class="text-danger" asp-validation-for="Key"></span>
                    </div>
                    <div class="form-group">
                      <label for="settingsValue">Settings Value</label>
                      <input name="settingsValue" value="@Model.Value" id="settingsValue" class="form-control">
                      <span class="text-danger" asp-validation-for="Value"></span>
                    </div>
                    <div class="text-danger" asp-validation-summary="ModelOnly"></div>
                    <button type="submit" class="btn btn-primary mr-2">Submit</button>
                    <a class="btn btn-light" asp-area="Admin" asp-controller="Settings" asp-action="Index">Cancel</a>
                  </form>
                </div>
              </div>
</section>