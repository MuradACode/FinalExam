@model Settings

<section class="container">
        <div class="card">
                <div class="card-body">
                  <h4 class="card-title">Feature - Details</h4>
<div class="d-flex justify-content-between">
    <a class="btn btn-primary" asp-area="Admin" asp-controller="Settings" asp-action="Index">Go Back</a>
    <a class="btn btn-success" asp-area="Admin" asp-controller="Settings" asp-action="Create">Create</a>
</div>
                  <div class="table-responsive">
                    <table class="table table-striped">
                      <thead>
                        <tr>
                          <th>
                            Key
                          </th>
                          <th>
                            Value
                          </th>
                        </tr>
                      </thead>
                      <tbody>
                             <tr>
                          <td class="py-1">
                            @Model.Key
                          </td>
                          <td class="py-1">
                            @Model.Value
                          </td>
                          <td>
    <a class="btn btn-warning mb-3" asp-area="Admin" asp-controller="Settings" asp-action="Update" asp-route-id="@Model.Id">Update</a>
    <a class="btn btn-danger mb-3" asp-area="Admin" asp-controller="Settings" asp-action="Delete" asp-route-id="@Model.Id">Delete</a>
                          </td>
                        </tr>
                      </tbody>
                    </table>
                  </div>
                </div>
              </div>
</section>